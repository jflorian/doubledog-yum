{
    "name": "doubledog-yum",
    "version": "1.1.0",
    "author": "John Florian",
    "license": "GPL-3.0",
    "summary": "A module to manage the yum/dnf package managers",
    "source": "https://www.doubledog.org/git/puppet-yum.git",
    "operatingsystem_support": [
        {
            "operatingsystem": "CentOS",
            "operatingsystemmajrelease": [
                "7"
            ]
        },
        {
            "operatingsystem": "Fedora",
            "operatingsystemrelease": [
                "28",
                "29"
            ]
        }
    ],
    "dependencies": [
        {
            "name": "doubledog/ddolib",
            "version_requirement": ">= 0.0.0 < 1.0.0"
        },
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": ">= 4.25.0 < 5.0.0"
        }
    ]
}
